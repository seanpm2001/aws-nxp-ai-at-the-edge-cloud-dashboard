    "babel-plugin-package-name-import": "^1.0.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.4.1",
    "change-case": "^3.0.2",
    "circular-dependency-plugin": "^5.0.2",
    "classnames": "^2.2.6",
    "commander": "^2.17.1",
    "compression-webpack-plugin": "^3.0.0",
    "faker": "^4.1.0",
