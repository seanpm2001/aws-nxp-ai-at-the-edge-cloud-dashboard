{
  "name": "root",
  "version": "1.0.0",
  "main": "/",
  "license": "MIT",
  "scripts": {
    "start": "cd ./src/client && quasar dev",
    "lint": "./node_modules/eslint/bin/eslint.js --ignore-path .gitignore",
    "fix": "yarn lint --fix",
    "deploy": "yarn configPrepare && cd ./src/client && quasar build && cd ../../ && yarn deployPrepare && ./node_modules/.bin/babel-node --presets=env,stage-0 ./src/aws/util/cfCli deploy",
    "update": "cd ./src/client && quasar build && cd ../../ && yarn deployPrepare && ./node_modules/.bin/babel-node --presets=env,stage-0 ./src/aws/util/cfCli update",
    "delete": "./node_modules/.bin/babel-node --presets=env,stage-0 ./src/aws/util/cfCli remove",
    "installAll": "cd ./src/client && yarn install && cd ../../ && yarn install",
    "makeFilenamesFile": "touch ./webpackCompressedFilenames.js && echo export default [] > ./webpackCompressedFilenames.js",
    "removeDistFolder": "rm -rf ./dist",
    "deployPrepare": "yarn makeFilenamesFile && yarn removeDistFolder",
    "configPrepare": "mkdir -p config && touch ./config/cfOutput.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
